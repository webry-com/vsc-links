{
  "name": "vscode-links",
  "displayName": "VSCode Links",
  "description": "Create custom links based on patterns.",
  "author": "Samuel Braun <sam@webry.com>",
  "publisher": "web-dev-sam",
  "license": "GPL-3.0-only",
  "version": "0.0.1",
  "homepage": "https://github.com/webry-com/vscode-links#readme",
  "engines": {
    "vscode": "^1.90.0"
  },
  "categories": [
    "Other"
  ],
  "bugs": {
    "email": "sam@webry.com",
    "url": "https://github.com/webry-com/vscode-links/issues"
  },
  "keywords": [
    "links",
    "tool",
    "url"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/webry-com/vscode-links"
  },
  "activationEvents": [
    "onStartupFinished"
  ],
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "vscode-links.activate",
        "title": "VSCode Links: Activate Extension"
      }
    ]
  },
  "capabilities": {
    "virtualWorkspaces": {
      "supported": false,
      "description": "Using VSCode Links is not possible in virtual workspaces."
    },
    "untrustedWorkspaces": {
      "supported": false,
      "description": "VSCode Links requires workspace trust because it loads configuration files and code from the workspace and executes it."
    }
  },
  "extensionKind": [
    "workspace"
  ],
  "scripts": {
    "vscode:prepublish": "pnpm run compile",
    "compile": "esbuild ./src/extension.ts --bundle --outfile=out/main.js --external:vscode --format=cjs --platform=node --minify",
    "watch": "tsc -watch -p ./",
    "pretest": "pnpm run compile && pnpm run lint",
    "lint": "eslint src --ext ts",
    "test": "vscode-test"
  },
  "devDependencies": {
    "@eslint/js": "^9.6.0",
    "@types/mocha": "^10.0.6",
    "@types/node": "18.x",
    "@types/vscode": "^1.90.0",
    "@vscode/test-cli": "^0.0.8",
    "@vscode/test-electron": "^2.3.9",
    "esbuild": "^0.21.5",
    "eslint": "^9.6.0",
    "globals": "^15.6.0",
    "prettier": "^3.3.2",
    "typescript": "^5.3.3",
    "typescript-eslint": "^7.14.1"
  },
  "dependencies": {
    "minimatch": "^9.0.5",
    "zod": "^3.23.8"
  }
}